{
    "manifest_version": 3,
    "name": "wikiwanderer",
    "version": "1.0",
    "description": "Get random Wikipedia articles and notifications.",
    "permissions": [
      "notifications",
      "background",
      "storage",
      "https://en.wikipedia.org/"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  